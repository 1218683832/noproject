<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Parallax_Animation_Attr">
        <!-- X方向上的位移 -->
        <attr name="translationXIn" format="float"/>
        <attr name="translationXOut" format="float"/>

        <!-- Y方向上的位移 -->
        <attr name="translationYIn" format="float"/>
        <attr name="translationYOut" format="float"/>

        <!-- Alpha -->
        <attr name="alphaIn" format="float"/>
        <attr name="alphaOut" format="float"/>
    </declare-styleable>
</resources>
